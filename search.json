[{"path":"https://mlr3forecast.mlr-org.com/authors.html","id":null,"dir":"","previous_headings":"","what":"Authors","title":"Authors and Citation","text":"Maximilian Mücke. Author, maintainer.","code":""},{"path":"https://mlr3forecast.mlr-org.com/authors.html","id":"citation","dir":"","previous_headings":"","what":"Citation","title":"Authors and Citation","text":"Mücke M (2024). mlr3forecast: Extending 'mlr3' Time Series Forecasting. R package version 0.0.1.","code":"@Manual{,   title = {mlr3forecast: Extending 'mlr3' to Time Series Forecasting},   author = {Maximilian Mücke},   year = {2024},   note = {R package version 0.0.1}, }"},{"path":"https://mlr3forecast.mlr-org.com/index.html","id":"mlr3forecast","dir":"","previous_headings":"","what":"Extending mlr3 to Time Series Forecasting","title":"Extending mlr3 to Time Series Forecasting","text":"Extending mlr3 time series forecasting. package early stage development considered experimental.","code":""},{"path":"https://mlr3forecast.mlr-org.com/index.html","id":"installation","dir":"","previous_headings":"","what":"Installation","title":"Extending mlr3 to Time Series Forecasting","text":"Install development version GitHub:","code":"# install.packages(\"pak\") pak::pak(\"mlr-org/mlr3forecast\")"},{"path":"https://mlr3forecast.mlr-org.com/index.html","id":"usage","dir":"","previous_headings":"","what":"Usage","title":"Extending mlr3 to Time Series Forecasting","text":"","code":"library(mlr3forecast) #> Loading required package: mlr3 library(mlr3learners)  dt = tsbox::ts_dt(AirPassengers) dt[, time := NULL] task = as_task_regr(dt, target = \"value\")  ff = Forecaster$new(   learner = lrn(\"regr.ranger\"),   lag = 1:3 ) ff$train(task) prediction = ff$predict(task) prediction #> <PredictionRegr> for 144 observations: #>     row_ids    truth response #>           1 432.0000 404.1487 #>           2 404.1487 450.5137 #>           3 450.5137 420.9816 #> ---                           #>         142 452.6298 454.5250 #>         143 454.5250 454.5353 #>         144 454.5353 445.7902 prediction = ff$predict_newdata(task, 3L) prediction #> <PredictionRegr> for 3 observations: #>  row_ids truth response #>        1    NA 404.1487 #>        2    NA 450.5137 #>        3    NA 420.9816 prediction = ff$predict(task, 142:144) prediction #> <PredictionRegr> for 3 observations: #>  row_ids    truth response #>        1 508.0000 498.0064 #>        2 498.0064 460.8071 #>        3 460.8071 445.5276 prediction$score(msr(\"regr.rmse\")) #> regr.rmse  #>  23.92435"},{"path":"https://mlr3forecast.mlr-org.com/reference/DataBackendTimeSeries.html","id":null,"dir":"Reference","previous_headings":"","what":"DataBackend for time series — DataBackendTimeSeries","title":"DataBackend for time series — DataBackendTimeSeries","text":"DataBackend data.table serves efficient -memory data base.","code":""},{"path":"https://mlr3forecast.mlr-org.com/reference/DataBackendTimeSeries.html","id":"super-classes","dir":"Reference","previous_headings":"","what":"Super classes","title":"DataBackend for time series — DataBackendTimeSeries","text":"mlr3::DataBackend -> mlr3::DataBackendDataTable -> DataBackendTimeSeries","code":""},{"path":"https://mlr3forecast.mlr-org.com/reference/DataBackendTimeSeries.html","id":"methods","dir":"Reference","previous_headings":"","what":"Methods","title":"DataBackend for time series — DataBackendTimeSeries","text":"mlr3::DataBackend$format() mlr3::DataBackend$print() mlr3::DataBackendDataTable$data() mlr3::DataBackendDataTable$distinct() mlr3::DataBackendDataTable$head() mlr3::DataBackendDataTable$missings()","code":""},{"path":"https://mlr3forecast.mlr-org.com/reference/DataBackendTimeSeries.html","id":"public-methods","dir":"Reference","previous_headings":"","what":"Public methods","title":"DataBackend for time series — DataBackendTimeSeries","text":"DataBackendTimeSeries$new()","code":""},{"path":"https://mlr3forecast.mlr-org.com/reference/DataBackendTimeSeries.html","id":"method-new-","dir":